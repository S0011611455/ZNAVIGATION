<mvc:View
   controllerName="com.wakefern.znavigation.controller.Home"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <Page title="{i18n>homePageTitle}" class="sapUiResponsiveContentPadding">
      <content>
         <Button id="navigateBtn" text="{i18n>ShowEmployeeOverview}"  press=".onNavToEmployeeOverview"   class="sapUiTinyMarginEnd"/>
         <Button id="displayNotFoundBtn" text="{i18n>DisplayNotFound}" press="onDisplayNotFound" class="sapUiTinyMarginEnd"/>
         <Button id="employeeListBtn" text="{i18n>ShowEmployeeList}" press=".onNavToEmployees" class="sapUiTinyMarginEnd"/>
         <Button id="productListBtn" text="{i18n>ShowProductList}" press=".onNavToProducts" class="sapUiTinyMarginEnd"/>
      </content>
   </Page>
</mvc:View>